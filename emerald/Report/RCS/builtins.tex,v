a128 51
{\small\it\begin{minipage}{\textwidth}\begin{tabbing}
xxx\=xxx\=xxx\=xxx\=xxx\=xxx\=xxx\=xxx\=xxx\=xxx\=xxx\=xxx\=xxx\=\+\kill%
  \kw{function}  getElement\/\LB{}\tn{Integer}\/\RB{} \returns{} \/\LB{}T\/\RB{}\+\\*{}%
    \cd{} get the element indexed by index, failing if index \\*{}%
    \cd{} out of range.\-\\*{}%
  \kw{operation} setElement\/\LB{}\tn{Integer}, T\/\RB{}\+\\*{}%
    \cd{} set the element, failing if index out of range\-\\*{}%
  \kw{function}  upperbound \returns{} \/\LB{}\tn{Integer}\/\RB{}\+\\*{}%
    \cd{} return the highest valid index, ub.\-\\*{}%
  \kw{function}  lowerbound \returns{} \/\LB{}\tn{Integer}\/\RB{}\+\\*{}%
    \cd{} return the lowest valid index, lb.\-\\*{}%
  \kw{function}  getSlice\/\LB{}\tn{Integer}, \tn{Integer}\/\RB{} \returns{} \/\LB{}NA\/\RB{}\+\\*{}%
    \cd{} return a new \tn{Array}, a, with lower bound lb, and upper bound ub,\\*{}%
    \cd such that for lb $<=$ i $<=$ ub\CO{} self.getElement\/\LB{}i\/\RB{} $==$ a.getElement\/\LB{}i\/\RB{}\\*{}%
    \cd{} fail if lb or ub is out of range.\-\\*{}%
  \kw{function}  getElement\/\LB{}\tn{Integer}, \tn{Integer}\/\RB{} \returns{} \/\LB{}NA\/\RB{}\+\\*{}%
    \cd{} Same as getSlice.\-\\*{}%
  \kw{operation} setSlice\/\LB{}\tn{Integer}, \tn{Integer}, RIS\/\RB{}\+\\*{}%
    \cd{} set the elements indexed by i for lb $<=$ i $<=$ ub, so that \\*{}%
    \cd{} for each such i\CO{} self.getElement\/\LB{}i\/\RB{} $==$ a.getElement\/\LB{}i\/\RB{}\\*{}%
    \cd{} fail if lb or ub is out of range.\-\\*{}%
  \kw{operation} setElement\/\LB{}\tn{Integer}, \tn{Integer}, RIS\/\RB{}\+\\*{}%
    \cd{} Same as setSlice.\-\\*{}%
  \kw{operation} slideTo\/\LB{}\tn{Integer}\/\RB{}\+\\*{}%
    \cd{} change the valid indices for self.  Assuming \\*{}%
    \cd{} the old indices ranged from lb to ub, the new \\*{}%
    \cd{} indices will range from i to i $+$ ub $-$ lb\-\\*{}%
  \kw{operation} addUpper\/\LB{}T\/\RB{}\+\\*{}%
    \cd{} extend the set of valid indices, changing ub to \\*{}%
    \cd{} ub $+$ 1, and setting the element indexed by the new\\*{}%
    \cd{} ub to be e.\-\\*{}%
  \kw{operation} removeUpper \returns{} \/\LB{}T\/\RB{}\+\\*{}%
    \cd{} return the element indexed by ub, after contracting \\*{}%
    \cd{} the set of valid indices to lb $<=$ i $<=$ ub $-$ 1.\-\\*{}%
  \kw{operation} addLower\/\LB{}T\/\RB{}\+\\*{}%
    \cd{} extend the set of valid indices, changing lb to \\*{}%
    \cd{} lb $-$ 1, and setting\\*{}%
    \cd{} the element indexed by the new lb to be e.\-\\*{}%
  \kw{operation} removeLower \returns{} \/\LB{}T\/\RB{}\+\\*{}%
    \cd{} return the element indexed by lb, after contracting \\*{}%
    \cd{} the set of valid indices to lb $+$ 1 $<=$ i $<=$ ub.\-\\*{}%
  \kw{function}  empty \returns{} \/\LB{}\tn{Boolean}\/\RB{}\+\\*{}%
    \cd{} \returns{} true if lb $==$ ub $+$ 1, which is true initially,\\*{}%
    \cd{} since initially lb $=$ 0, and ub $=$ -1.\-\\*{}%
  \kw{operation} catenate\/\LB{}a \CO{} RIS\/\RB{} \returns{} \/\LB{}r \CO{} NA\/\RB{}\+\\*{}%
    \cd{} extend the range of valid indices from lb .. ub to \\*{}%
    \cd{} lb .. ub $+$ a.length.  Set these new elements to refer\\*{}%
    \cd{} to the elements of a so that for a.lb $<=$ i $<=$ a.ub, \\*{}%
    \cd{} where oub is the old value of self.ub\CO{}\\*{}%
    \cd{}     a.getElement\/\LB{}i\/\RB{} $==$ self.getElement\/\LB{}oub $+$ i $-$ 1\/\RB{}
\end{tabbing}\end{minipage}}

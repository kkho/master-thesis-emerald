#!/bin/csh -f
@ i = 1
unset passed
set old = `tcsh latestversion`
while ($i <= 3)
    echo "Regression pass $i"
    tcsh useversion $old
    #rm -f bestCP

    (tcsh emmake bestCP && emx -C bestCP) || break
    tcsh useversion best
    tcsh emmake ftffs EMC=xemc || break
    tcsh newversion
    set new = `tcsh latestversion`

    if { cmp -s CP_q$old CP_q$new } then
	set passed
	echo Regression done
	break
    endif
    set old = $new
    @ i++
end
if (! $?passed ) then
    echo 'Regression failed\!'
endif
